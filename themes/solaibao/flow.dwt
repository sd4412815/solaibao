        {literal}
        function checkLoginForm(frm) {
          if (Utils.isEmpty(frm.elements['username'].value)) {
              layer.msg(username_not_null, function(){});
            return false;
          }

          if (Utils.isEmpty(frm.elements['password'].value)) {
              layer.msg(password_not_null, function(){});
            return false;
          }

          return true;
        }

        function checkSignupForm(frm) {
          if (Utils.isEmpty(frm.elements['username'].value)) {
              layer.msg(username_not_null, function(){});
            return false;
          }

          if (Utils.trim(frm.elements['username'].value).match(/^\s*$|^c:\\con\\con$|[%,\'\*\"\s\t\<\>\&\\]/))
          {
              layer.msg(username_invalid, function(){});
            return false;
          }

/*          if (Utils.isEmpty(frm.elements['email'].value)) {
              layer.msg(flow_no_payment, function(){});

              alert(email_not_null);
            return false;
          }*/

/*          if (!Utils.isEmail(frm.elements['email'].value)) {
              layer.msg(flow_no_payment, function(){});
              alert(email_invalid);
            return false;
          }*/

          if (Utils.isEmpty(frm.elements['password'].value)) {
              layer.msg(password_not_null, function(){});
            return false;
          }

          if (frm.elements['password'].value.length < 6) {
              layer.msg(password_lt_six, function(){});
            return false;
          }

          if (frm.elements['password'].value != frm.elements['confirm_password'].value) {
              layer.msg(password_not_same, function(){});
            return false;
          }
          return true;
        }
        {/literal}
