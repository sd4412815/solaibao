    function selectShipping(){
			
        var shipping_id = $('#shipping option:selected') .val();

        var country=$('#country').html();
        var province=$('#province').html();
        var city=$('#city').html();
        var district=$('#district').html();
         // Ajax.call('flow.php?step=select_shipping', 'shipping=' + shipping_id, orderShippingSelectedResponse, 'GET', 'JSON',true,true);
