          type: "GET",
          dataType:"JSON",
          async:true,

          success:function(data){ 
            orderShippingSelectedResponse(data);
          },
          
        });
		
         Ajax.call( 'shipping.php?act=get_shipping_info', 'id=' + shipping_id, shipping_callback , 'GET', 'TEXT', true, true );
		
        Ajax.call( 'shipping.php?act=shipping_fee', 'id=' + shipping_id+'&country='+country+'&province='+province+'&city='+city+'&district='+district, shipping_fee_callback , 'GET', 'TEXT', true, true );
		    
    }

    function shipping_callback(result){
        $('#shipping_info').html(result);
		
    }
	
    function shipping_fee_callback(result){
        $('#shipping_fee').html('¥ '+result+'.00元');
    }

    function idCard(){
        var reg=/^[1-9]{1}[0-9]{14}$|^[1-9]{1}[0-9]{16}([0-9]|[xX])$/;
